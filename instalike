var simulateClick = function (elem) {
    // Create our event (with options)
    var evt = new MouseEvent('click', {
        bubbles: true,
        cancelable: true,
        view: window
    });
    // If cancelled, don't dispatch our event
    var canceled = !elem.dispatchEvent(evt);
};

var someLink = document.querySelector('a._de018.coreSpriteRightPaginationArrow');
var someLink2 = document.querySelector('span._soakw.coreSpriteLikeHeartOpen');

setInterval(function(){
	simulateClick(someLink);
	simulateClick(someLink2);
	
}, 3000);
